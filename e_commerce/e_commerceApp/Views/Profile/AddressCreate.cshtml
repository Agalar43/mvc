@model AddressDtoForInsertion

<div class="row">

    <div class="col-md-2 my-4">

        <partial name="_ProfileSideBar" />

    </div>
    <div class="col-md-9 mx-3">
        <div class="row my-2">
            <div class="text-center">
                <div class="display-6">
                    <h1>Create</h1>
                </div>
            </div>

            <form asp-controller="Profile" asp-action="AddressCreate" method="post" enctype="multipart/form-data">
                <div class="form-group my-3 ">
                    <label asp-for="CityName"></label>
                    <input asp-for="CityName" class="form-control" />
                </div>
                <div class="form-group my-3 ">
                    <label asp-for="DistrictName"></label>
                    <input asp-for="DistrictName" class="form-control" />
                </div>
                <div class="form-group my-3 ">
                    <label asp-for="AddressName"></label>
                    <input asp-for="AddressName" class="form-control" />
                </div>
                <div class="form-group my-3 ">
                    <label asp-for="ZipCode"></label>
                    <input asp-for="ZipCode" class="form-control" />
                </div>
                <button class="btn btn-success" type="submit">
                    <i class="fa fa-save"></i>
                    Save
                </button>
            </form>
        </div>

    </div>
